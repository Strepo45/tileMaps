//portal function///
function portals (portal_number: number) {
    if (portal_number == 1) {
        portal_BOSSL1 = sprites.create(img`
            ......ffff.ffffff.......
            ......fabbfccccbff......
            .....faaaacccccaaff.....
            ....faacaccaccccaaf.....
            ....facccccaaccccaaf....
            ....fccccccbaccccaaff...
            ..ffacccccccccccccaaff..
            ..fccccccccccccccccaaff.
            .ffccccccccccccccccccff.
            .facccccccccaacccccccff.
            .faccaacccccccccccccccf.
            .facccacccccccccccccccf.
            .faccccccccccccacccccaf.
            .faaccccccbacccccccccaf.
            .ffaccccccccccccccacaf..
            ..faccccccccccccccccaf..
            ..faacccccccbaccccccaf..
            .afaaccccacccccccccafa..
            a..faccccccccccccccaf.a.
            ..afccccccccccccccccf...
            .a.faaccccccccccccccf.a.
            ...ffaacccccccccccaff...
            .a..fffccaaacccccffa....
            ......ffffaacccfff..a...
            `, SpriteKind.portal)
        portal_BOSSL1.setScale(3, ScaleAnchor.Middle)
        tiles.placeOnRandomTile(portal_BOSSL1, assets.tile`myTile22`)
    } else if (portal_number == 2) {
        portal_L2 = sprites.create(img`
            ........fffffffff.......
            .......ff4424442ff......
            ......f2422242242ff.....
            .....ff24222422422ff....
            ....f24222224224422ff...
            ...f4422242242222222f...
            ..ff4422422222222224ff..
            ..f442222222222222224f..
            .ff242222222222222224f..
            .f2242222224222242222ff.
            .f22422222422224222224f.
            .fff222224222222222224f.
            .f42222222422242222222f.
            .f44222224222222242222f.
            .f24222222222222422224f.
            .ff2422222222224422224f.
            ..f2422222222224222244f.
            ..f222222222222222244ff.
            ..ff2222222222222244ff..
            ...f222222222422224ff.2.
            ..4.f2422222442244ff..44
            .2...444422442224ff.....
            ....44ff4224222fff..4...
            ....4..fffffffff........
            `, SpriteKind.portal)
        portal_L2.setScale(3, ScaleAnchor.Middle)
        tiles.placeOnRandomTile(portal_L2, assets.tile`myTile31`)
    } else if (portal_number == 3) {
        portal_lvl2part2 = sprites.create(img`
            ...fffffffffffffffff....
            ...f889999999999688f....
            ...f869999999999998f....
            ...f899999911999998f....
            ...f699999999999998f....
            ...f9999119999999988f...
            ...f99919999999999688f..
            ...f99999999991119998f..
            ...f99999911199999998f..
            ...f99999199999999998f..
            ..ff99999999999911986f..
            ..f999999999999199866f..
            ..f999999999911999866f..
            ..f889f9999119999986f...
            ..ff89f9999999999986f...
            ...f6999999999999986f...
            ...f6999999911999988f...
            ...f8999999199999998f...
            ...f8999119999999998f...
            ...f8991999999999198ff..
            ...f89199199999119986f..
            ...f89991999991999988f..
            ...f99999999999999998f..
            ...ff99999999999999ff...
            `, SpriteKind.portal)
        portal_lvl2part2.setScale(2.4, ScaleAnchor.Middle)
        tiles.placeOnRandomTile(portal_lvl2part2, assets.tile`myTile44`)
    } else if (portal_number == 4) {
        portal_BOss2 = sprites.create(img`
            ........................
            ........................
            ........................
            .......999f99999........
            .....99888ff888899......
            ....9f89999999191f9f....
            ..f9ff91199999981ff9f...
            ..fcf911999999911f8cf...
            ..c8f9919999999999f89...
            ..98f9199999999999f89...
            ..98f1199999999999f89...
            ..98f9999999919999f89...
            ..9f999999991999999f9...
            ..ff999999891999999ff...
            ..f89999999199999f9ff...
            ..f8199999999999f99f9...
            .f98919999999991f9989...
            .ffc8f91999999919f8cf...
            ..fcff99199999991ffcf...
            ....cff9199999919fcf..9.
            ..9..9988888ff8899f.....
            .......99fff9999........
            ........................
            ........................
            `, SpriteKind.portal)
        portal_BOss2.setScale(3, ScaleAnchor.Middle)
        tiles.placeOnRandomTile(portal_BOss2, assets.tile`myTile105`)
    } else {
    	
    }
}






//places rv//
function rv () {
    RV2 = sprites.create(assets.image`myImage`, SpriteKind.RV)
    RV2.setScale(1.5, ScaleAnchor.Middle)
    tiles.placeOnRandomTile(RV2, assets.tile`myTile4`)
}



//on overlap to exit a level//
scene.onOverlapTile(SpriteKind.Player, assets.tile`myTile105`, function (sprite5, location3) {
    current_level += 1
    change_level(current_level)
    current_portal += 1
    portals(current_portal)
})





